<?xml version="1.0" encoding="UTF-8" ?>
<project name="DemoUI" default="replace" basedir=".">
    <description>
        Replaces localhost url with the supplied url
    </description>
    <target name="replace">
        <replace file="js/main.js" token="http://localhost:9000" value="${ONS_BI_UI_API_URL}" />
        <replace file="js/main.js" token="$API_KEY" value="${ONS_BI_UI_API_KEY}" />
    </target>
</project>
